  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "header-max-length": [
        2,
        "always",
        72
      ],
      "body-max-line-length": [
        2,
        "always",
        80
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn format:staged",
      "yarn lint:license:staged",
      "yarn lint:scripts:staged"
    ],
    "*.scss": [
      "yarn format:staged",
      "yarn lint:license:staged"
    ],
    "*.{css,md}": [
      "yarn format:staged"
    ]
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "printWidth": 130,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
